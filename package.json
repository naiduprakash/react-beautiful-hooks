{
  "name": "react-beautiful-hooks",
  "description": "A collection of beautiful reusable custom react hooks",
  "author": "Prakash Naidu",
  "keywords": [
    "react",
    "hooks",
    "react-hooks",
    "custom-hooks",
    "react-custom-hooks"
  ],
  "version": "0.1.5",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/naiduprakash/react-beautiful-hooks.git"
  },
  "dependencies": {},
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "build": "rimraf dist && cross-env-shell NODE_ENV=production babel src/lib --out-dir dist --copy-files",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "rimraf": "^3.0.2"
  }
}
